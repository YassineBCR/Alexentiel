{
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/mariage",
      "destination": "/mariage.html"
    },
    {
      "source": "/autre-evenement",
      "destination": "/autre-evenement.html"
    },
    {
      "source": "/autre-evenements",
      "destination": "/autre-evenement.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

